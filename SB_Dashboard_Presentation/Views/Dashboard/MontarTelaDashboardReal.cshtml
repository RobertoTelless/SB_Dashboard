                                                                    <small style="cursor: pointer; text-decoration: underline; color: #337ab7;" onclick="mostrarDesc('Descrição do que significa este indicador')">Descrição</small>
                                                                    <br />
                                                                    <br />
                                                                    @if ((Int32)ViewBag.FalhaPC == 0)
